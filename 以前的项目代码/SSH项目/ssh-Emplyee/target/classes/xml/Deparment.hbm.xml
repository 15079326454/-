<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hgkj.model.entity.Deparment" table="deparment" schema="test">
        <id name="dId">
            <column name="DId" sql-type="int(11)"/>
            <generator class="native"/>
        </id>
        <property name="dName">
            <column name="DName" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="dDesc">
            <column name="DDesc" sql-type="varchar(255)" not-null="true"/>
        </property>
<set name="employees" lazy="false" inverse="true">
    <key column="DId"/>
    <one-to-many class="com.hgkj.model.entity.Employee"/>
</set>
    </class>
</hibernate-mapping>